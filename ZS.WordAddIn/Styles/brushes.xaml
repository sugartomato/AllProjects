<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- Brushes -->
    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0" x:Key="brushWS1">
        <GradientStop Color="White" Offset="0.0"></GradientStop>
        <GradientStop Color="Silver" Offset="0.5"></GradientStop>
    </LinearGradientBrush>


    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" x:Key="brushWhiteBlue">
        <GradientStop Color="White" Offset="0.0"></GradientStop>
        <GradientStop Color="LightBlue" Offset="0.5"></GradientStop>
        <GradientStop Color="White" Offset="1.0"></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush EndPoint="0,1" x:Key="brushWhiltBlue2">
        <GradientStop Color="White" Offset="0.0"></GradientStop>
        <GradientStop Color="Blue" Offset="0.5"></GradientStop>
        <GradientStop Color="White" Offset="1.0"></GradientStop>
    </LinearGradientBrush>


    <LinearGradientBrush EndPoint="1,1" x:Key="brushMColor1">
        <GradientStop Color="LightBlue" Offset="0.0"></GradientStop>
        <GradientStop Color="LightGray" Offset="0.3"></GradientStop>
        <GradientStop Color="LightGreen" Offset="0.6"></GradientStop>
        <GradientStop Color="LightCyan" Offset="0.9"></GradientStop>
    </LinearGradientBrush>
</ResourceDictionary>